<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="XFCogServices.AnalysisPage" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <ListView ItemsSource="{Binding AnalysisResult}">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding itemName}" Detail="{Binding itemDesc}"/>
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>


</ContentPage>